#include <stdio.h>
#include <math.h>
int main()
{
    float f,m, r,c ;
    printf (" Escribir un numero real en grados Faherenheit \n \n");
    scanf ("%f", &f);
    // se restan los grados Faherenheit con 32 
    r= f-32;
    // Se multiplica el resultado dado arriba entre 5
    m= 5 * r;
    // Por ultimo se divide entre 9, para poder convertir los grados Faherenheit a Celsius
    c= m/9;
    printf ( " \n La conversion de los gardos Faherenheit a Celsius es %f \n " , c);
    return 0;
}
Algoritmo ejercicio _ 4

	Definir parcial, taller, quiz, ejercicioClase, quiz2, exposicion, nota, notaFinal Como real	

    Imprimir "Para poder calcular tu nota final por favor ayúdanos con los siguientes datos"

	

    Imprimir "Ingrese la calificación obtenida en el Parcial"

    Leer parcial

	

    Imprimir "Ingrese la calificación obtenida en el Taller"

    Leer taller

	

    Imprimir "Ingrese la calificación obtenida en el Quiz"

    Leer quiz

	

    Imprimir "Ingrese la calificación obtenida en el Ejercicio de Clase"

    Leer ejercicioClase

	

    Imprimir "Ingrese la calificación obtenida en el Quiz 2"

    Leer quiz2

	

    Imprimir "Ingrese la calificación obtenida en la Exposición"

    Leer exposicion

	

    notaFinal = (parcial + taller + quiz + ejercicioClase + quiz2 + exposicion) / 6

	

    Imprimir "La nota final es: ", notaFinal

	

